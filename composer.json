{
    "name": "salvaworld/envoyer-sdk",
    "description": "Laravel envoyer SDK.",
    "keywords": [
        "salvaworld",
        "envoyer",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Salva",
            "email": "info@salvaworld.com",
            "homepage": "https://salvaworld.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "SalvaWorld\\Envoyer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
